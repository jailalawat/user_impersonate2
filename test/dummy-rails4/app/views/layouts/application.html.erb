<!DOCTYPE html>
<html>
<head>
  <title>Dummy</title>
  <%= stylesheet_link_tag    "application", :media => "all" %>
  <%= javascript_include_tag "application" %>
  <%= csrf_meta_tags %>
</head>
<body>
  <% if current_staff_user %>
    <%= render 'user_impersonate/header' %>
  <% end %>

  <div id="container">
    <p class="notice"><%= flash[:notice] %></p>
    <p class="alert"><%= flash[:error] %></p>

    <%= yield %>
  </div>

</body>
</html>
